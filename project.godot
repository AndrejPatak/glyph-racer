config_version=5

[application]

config/name="glyph-racer"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Colorizer="*res://colorizer.tscn"
TimeTracker="*res://time_tracker.tscn"
Music="*res://music.tscn"
Settings="*res://general_settings.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg")

[global_group]

track_label="This group tracks buttons to assign a string for the time achieved on that track"
track_button="Group to track all buttons that should open a track"
player="The group to detect player collisions"
Smasher="Group to keep track of smashers in a track"
sound_effect="Group used to set the volume for sound effects"

